package com.google.interview.questions.trees;

class BinaryTree {

	int data;
	BinaryTree left;
	BinaryTree right;

	public BinaryTree(int data) {
		this.data = data;
	}
}

public class TopViewOfBinaryTree {
	public static void main(String[] args) {
		BinaryTree binTree1 = new BinaryTree(3);
		BinaryTree binTree2 = new BinaryTree(5);
		BinaryTree binTree3 = new BinaryTree(2);
		BinaryTree binTree4 = new BinaryTree(1);
		BinaryTree binTree5 = new BinaryTree(4);
		BinaryTree binTree6 = new BinaryTree(3);
		BinaryTree binTree7 = new BinaryTree(3);
		BinaryTree binTree8 = new BinaryTree(3);
		BinaryTree binTree9 = new BinaryTree(3);
		binTree1.left = binTree2;
		binTree1.right = binTree3;

	}

}
